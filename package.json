{
  "name": "material-community-components",
  "version": "6.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:lib:scss": "scss-bundle -c scss-bundle.config.json",
    "docs": "ng deploy --base-href=https://tiaguinho.github.io/material-community-components/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p src/lib/package.json && yarn build:lib:scss && cp README.md src/lib/dist/"
  },
  "private": true,
  "peerDependencies": {
    "core-js": "^3.1.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular-devkit/core": "^9.1.0",
    "@angular/animations": "^10.0.2",
    "@angular/cdk": "^9.2.0",
    "@angular/cli": "^9.1.0",
    "@angular/common": "^10.0.2",
    "@angular/compiler": "^10.0.2",
    "@angular/compiler-cli": "^10.0.2",
    "@angular/core": "^10.0.2",
    "@angular/forms": "^10.0.2",
    "@angular/language-service": "^10.0.2",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "^10.0.2",
    "@angular/platform-browser-dynamic": "^10.0.2",
    "@angular/router": "^10.0.2",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.9.5",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.2.2",
    "conventional-changelog": "^3.1.18",
    "core-js": "^3.6.4",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "~1.5.3",
    "ng-packagr": "^9.1.0",
    "protractor": "~5.4.3",
    "rxjs": "^6.5.4",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.0",
    "typescript": "^3.9.5",
    "uglify-es": "^3.3.9",
    "validate-commit-msg": "^2.14.0",
    "zone.js": "~0.10.3"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "release",
        "ci",
        "build"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": ""
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "scss-bundle": "^2.5.1",
    "tslib": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  }
}